void circulos(float posX, float posY, float tamX, float tamY) { 
  fill(0);
  ellipse(posX, posY ,240, 240);
  fill(255);
  ellipse(posX, posY ,200, 200);
  fill(0);
  ellipse(posX, posY,160, 160);
  fill(255);
  ellipse(posX, posY,120, 120);
  fill(0);
  ellipse(posX, posY,80, 80);
  fill(255);
  ellipse(posX, posY,40, 40);
}
